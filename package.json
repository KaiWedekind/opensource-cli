{
  "name": "opensource-cli",
  "version": "1.0.1",
  "description": "",
  "main": "cli.js",
  "template": {
    "repository": "https://github.com/KaiWedekind/opensource-template.git"
  },
  "preferGlobal": true,
  "bin": {
    "os-temp": "cli.js"
  },
  "scripts": {
    "prepush": "npm run validate",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run validate",
    "commit-m": "git-cz",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "watch:test": "mocha --watch",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "validate": "npm-run-all --parallel lint test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "env": [
    "mocha"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "camelcase": "4.1.0",
    "chalk": "2.1.0",
    "cli-spinners": "1.0.0",
    "co": "4.6.0",
    "co-prompt": "1.0.0",
    "commander": "2.11.0",
    "fs-extra": "4.0.1",
    "ora": "1.3.0",
    "require-dir": "0.3.2",
    "shelljs": "0.7.8",
    "yargs": "8.0.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-istanbul": "4.1.4",
    "babel-preset-env": "1.6.0",
    "babel-register": "6.26.0",
    "chai": "4.1.1",
    "codecov": "2.3.0",
    "commitizen": "2.9.6",
    "cross-env": "5.0.5",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.3.2",
    "eslint-plugin-import": "2.7.0",
    "husky": "0.14.3",
    "json-loader": "0.5.7",
    "mocha": "3.5.0",
    "npm-run-all": "4.1.1",
    "nyc": "11.1.0",
    "rimraf": "2.6.1",
    "semantic-release": "^7.0.2",
    "validate-commit-msg": "2.14.0",
    "webpack": "3.5.5"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 0,
    "functions": 0,
    "lines": 0,
    "statements": 0,
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "instrument": false,
    "sourceMap": false,
    "require": [
      "babel-register"
    ],
    "include": [
      "src"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KaiWedekind/opensource-cli.git"
  },
  "author": "Kai Wedekind <wedekind.kai@googlemail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KaiWedekind/opensource-cli/issues"
  },
  "homepage": "https://github.com/KaiWedekind/opensource-cli#readme'"
}
